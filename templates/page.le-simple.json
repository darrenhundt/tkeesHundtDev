    "rich_text_9E3WTC": {
      "type": "rich-text",
      "blocks": {
        "text_jzfqLE": {
          "type": "text",
          "settings": {
            "text": "<p><strong>ENCORE?  COME AGAIN?<\/strong><\/p>",
            "enlarge_text": true
          }
        },
        "text_4f9Hd3": {
          "type": "text",
          "settings": {
            "text": "<p>I thought you were done with clothing!?!<\/p>",
            "enlarge_text": true
